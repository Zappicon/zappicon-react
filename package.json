{
  "name": "@zappicon/react",
  "version": "0.0.1",
  "description": "A comprehensive React icon library with multiple variants (filled, outline, duotone, light, regular)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "icons",
    "react",
    "svg",
    "components",
    "zappicon"
  ],
  "author": "Haroun Abidi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappicon/zappicon-react.git"
  },
  "bugs": {
    "url": "https://github.com/zappicon/zappicon-react/issues"
  },
  "homepage": "https://zappicon.com",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "clean": "rm -rf dist && rm -rf src/icons",
    "generate": "node scripts/generate.mjs",
    "svgr": "svgr --out-dir src/icons icons/*.svg && pnpm run generate",
    "build": "pnpm run clean && pnpm run svgr && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@svgr/cli": "^8.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.9",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jiti": "^2.5.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-filesize": "^10.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.1-alpha.4"
  }
}
